@model List<PhysicManagement.Model.PhysicTreatmentPlanHostory>

@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <div class="row">
        <div class="col-lg-12 col-md-12">
            <div class="card">
                <div class="card-header card-header-info">
                    <h4 class="card-title">لیست اطلاعات نقشه درمان فیزیکی</h4>                    
                </div>
                <div class="card-body table-responsive">
                    <table class="table table-hover">
                        <thead class="text-warning">
                            <tr>
                                <th>ردیف</th>
                                <th>شناسه نقشه درمان فیزیکی</th>
                                <th>یادداشت</th>
                                <th>تغییر تاریخ</th>
                                <th>کاربر</th>
                                <th>دارای هشدار است</th>
                                <th>دکتر است</th>
                            </tr>
                        </thead>
                        <tbody>
                            @{
                                int Index = 1;
                                foreach (var physicTreatmentPlanHostory in Model)
                                {
                                    <tr>
                                        <td>@Index</td>
                                        <td>@physicTreatmentPlanHostory.PhysicTreatmentPlanId</td>
                                        <td>@physicTreatmentPlanHostory.Note</td>
                                        <td>@physicTreatmentPlanHostory.ChangeDate</td>
                                        <td>@physicTreatmentPlanHostory.ActionUser</td>
                                        <td>@physicTreatmentPlanHostory.HasAlarm</td>
                                        <td>@physicTreatmentPlanHostory.IsDoctor</td>
                                    </tr>
                                    Index++;
                                }

                            }


                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

